<% include ../views/partials/header %> 


<div class="row">
<div class="container">
  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"><img style="height:inherit;padding-top:40px" src="../images/faq.jpeg" id="faqimage"></div>
  
  <br>
<div class="jumbotron">
  </div>
  <div id="hello2">
    <p>
        <br><br>
          
        <% var keys= Object.keys(faq) %>
        <% keys.forEach(function(key){ %>
          <b><%= faq[key].question %></b><br>
          <%= faq[key].answer %>
        <br><br>
        <% }) %>
    
</p>
 
</div>
  </div>

<div class="container col s12 col l6 col m6 col xs12">
	<div class="jumbotron">
	<h2 align="center">Still have a querry? Ask a question</h2>
    </div>
    
  <form id="contactForm" >
<!--Validation -->
    <label for="name">Name</label>
    <input type="text" id="name" placeholder="Your name..">
                  
     <label for="email">Email</label>
    <input type="text" id="email" placeholder="Your email..">
    
    <label for="phone">Cell Phone</label>
    <input type="text" id="phone" placeholder="Your Phone..">
    
    <label for="subject">Message</label>
    <textarea id="subject"  placeholder="Ask your question.." style="height:200px"></textarea>
    <input type="submit" value="Submit"  >
    

  </form>
</div>

<footer class="container">
    <div class="container clearfix">
